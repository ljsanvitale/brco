
<div class='main_articles'>
    <ul id="articles">
      <% @authors.each do |author| %>
        <li>
          <%= link_to author.author_name, admin_author_path(author), class: 'article_title', id: "article_#{author.id}" %>
        </li>
      <% end %>
    </ul>
    <%= link_to "Create a New Author", new_admin_author_path, class: "new_article" %>
</div>
